   "source": [
    "with open(\"input.txt\", \"r\") as f:\n",
    "    data = f.readlines()\n",
    "    \n",
    "curr_max = 0;\n",
    "curr_sum = 0;\n",
    "\n",
    "for d in data:\n",
    "    if d == \"\\n\":\n",
    "        curr_max = max(curr_sum, curr_max)\n",
    "        curr_sum = 0\n",
    "    else:\n",
    "        curr_sum += int(d)\n",
    "        \n",
    "print(curr_max)\n",
    "        "
   ]
  }
